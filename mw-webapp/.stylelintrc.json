{
  "extends": [
    "stylelint-config-idiomatic-order"
  ],
  "plugins": [
    "@stylistic/stylelint-plugin",
    "stylelint-scss"
  ],
  "customSyntax": "postcss-scss",
  "rules": {
    "@stylistic/value-list-comma-newline-after": null,
    "@stylistic/declaration-colon-newline-after": null
  }
}
