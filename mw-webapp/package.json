{
  "name": "mw-webapp",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.tsx",
  "type": "module",
  "scripts": {
    "start": "vite --host",
    "build": "vite build && echo \"Copying _redirects for netlify\" && cp ./deploy/_redirects ./build/_redirects",
    "api:generate": "pnpm concurrently \"pnpm run api:generate:general\" \"pnpm run api:generate:chat\"",
    "api:generate:general": "openapi-generator-cli generate -g typescript-fetch -i http://localhost:8000/docs/doc.json -o ./src/apiAutogenerated/general -t ./openApiTemplates",
    "api:generate:chat": "openapi-generator-cli generate -g typescript-fetch -i http://localhost:8001/docs/doc.json -o ./src/apiAutogenerated/chat -t ./openApiTemplates",    
    "serve": "vite preview --port 5173 --host 0.0.0.0",
    "lint-check": "eslint .",
    "lint-fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "format": "prettier --write",
    "type-check": "tsc",
    "test": "vitest --run --reporter=verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:component:open": "cypress open --component",
    "cypress:component:run": "cypress run --component --spec ./src/component/all.cy.tsx",
    "cypress:e2e:open": "cypress open",
    "cypress:e2e:run": "cypress run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/themes": "2.0.3",
    "@tanstack/react-table": "8.12.0",
    "chart.js": "^4.4.1",
    "clsx": "2.1.0",
    "cypress-real-events": "^1.12.0",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "pdfmake": "^0.2.9",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-ga4": "^2.1.0",
    "react-markdown": "9.0.1",
    "react-router-dom": "6.22.0",
    "remark-gfm": "^4.0.0",
    "sass": "1.70.0",
    "toastify-js": "1.12.0",
    "typescript": "5.3.3",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "web-vitals": "3.5.2"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "21.1.0",
    "@storybook/addon-essentials": "^7.6.15",
    "@storybook/addon-interactions": "^7.6.15",
    "@storybook/addon-links": "^7.6.15",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "^7.6.15",
    "@storybook/react": "^7.6.15",
    "@storybook/react-vite": "^7.6.15",
    "@storybook/testing-library": "0.2.2",
    "@stylistic/eslint-plugin": "^1.6.1",
    "@stylistic/stylelint-plugin": "^2.0.0",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.10.0",
    "@types/pdfmake": "^0.2.9",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@types/react-helmet": "^6.1.11",
    "@types/toastify-js": "1.12.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "^2.0.2",
    "@vitest/ui": "^2.0.1",
    "@yarnaimo/eslint-config": "0.4.4",
    "cypress": "^13.7.2",
    "dotenv": "^16.4.5",
    "envalid": "8.0.0",
    "eslint": "8.56.0",
    "eslint-plugin-jsdoc": "48.1.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-vitest": "^0.4.1",
    "husky": "9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "15.2.2",
    "postcss-scss": "4.0.9",
    "storybook": "7.6.19",
    "stylelint": "16.2.1",
    "stylelint-config-idiomatic-order": "10.0.0",
    "stylelint-scss": "6.3.0",
    "vite": "5.2.11",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "^2.0.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm run lint-fix"
    ],
    "**/*.{scss,module.scss}": [
      "pnpm run format --",
      "pnpm run stylelint-fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "extends": "../tsconfig.json",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
