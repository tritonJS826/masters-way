{
  "name": "mw-training",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "make build",
    "start": "nodemon --watch . --exec 'make start' --signal SIGTERM --ext go,json,yaml,sql,env,sh",
    "serve": "make start",
    "test:integration": "make test",
    "installAll": "make install",
    "swag": "make swag",
    "db:run": "make db-run",
    "db:generate": "make db-schema",
    "help": "make help",
    "api:generate": "pnpm concurrently \"pnpm run api:generate:training\"",
    "api:generate:training": "protoc --proto_path=./proto --experimental_allow_proto3_optional --go_out=pkg/grpcAutogenerated/training --go_opt=paths=source_relative --go-grpc_out=pkg/grpcAutogenerated/training --go-grpc_opt=paths=source_relative proto/training.proto",
    "api:generate:test-websocket": "openapi-generator-cli generate -g typescript-fetch -i ../mw-test-websocket/docs/swagger.json -o ./src/apiAutogenerated/testWebsocket -t ./openApiTemplates"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "3.1.4"
  }
}
