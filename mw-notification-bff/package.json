{
  "name": "mw-notification-bff",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "make build",
    "start": "nodemon --watch . --exec 'make start' --signal SIGTERM --ext go,json,yaml,sql,env,sh",
    "serve": "make start",
    "test:integration": "make test",
    "installAll": "make install",
    "swag": "make swag",
    "help": "make help",
    "api:generate": "pnpm concurrently \"pnpm run api:generate:mw-notification\" \"pnpm run api:generate:mw-notification-bff\" ",
    "api:generate:mw-notification": "protoc --proto_path=../mw-notification/proto --go_out=pkg/grpcAutoGenerated/notification --go_opt=paths=source_relative --go-grpc_out=pkg/grpcAutoGenerated/notification --go-grpc_opt=paths=source_relative ../mw-notification/proto/notification.proto",
    "api:generate:mw-notification-bff": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-notification-bff/docs/swagger.json -o ./apiAutogenerated/mw-notification-bff"
  },
