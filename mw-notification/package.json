{
  "name": "mw-notification",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "make build",
    "start": "nodemon --watch . --exec 'make start' --signal SIGTERM --ext go,json,yaml,sql,env,sh",
    "serve": "make start",
    "test:integration": "make test",
    "installAll": "make install",
    "db:run": "make db-run",
    "db:generate": "make db-schema",
    "help": "make help",
    "api:generate": "pnpm concurrently \"pnpm run api:generate:notification\"",
    "api:generate:notification": "protoc --proto_path=./proto --go_out=pkg/grpcAutoGenerated/notification --go_opt=paths=source_relative --go-grpc_out=pkg/grpcAutoGenerated/notification --go-grpc_opt=paths=source_relative proto/notification.proto"
  },
