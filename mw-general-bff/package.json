{
  "name": "mw-general-bff",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "make build",
    "start": "nodemon --watch . --exec 'make start' --signal SIGTERM --ext go,json,yaml,sql,env,sh",
    "serve": "make start",
    "test:integration": "make test",
    "installAll": "make install",
    "swag": "make swag",
    "help": "make help",
    "api:generate": "pnpm concurrently \"pnpm run api:generate:general\" \"pnpm run api:generate:mw-general-bff\" \"pnpm run api:generate:mw-storage\" \"pnpm run api:generate:mw-notification\" \"pnpm run api:generate:mw-chat\" \"pnpm run api:generate:mw-chat-websocket\" \"pnpm run api:generate:mw-mail\" \"pnpm run api:generate:mw-training\"",
    "api:generate:general": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-server/docs/swagger.json -o ./apiAutogenerated/general",
    "api:generate:mw-general-bff": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-general-bff/docs/swagger.json -o ./apiAutogenerated/mw-general-bff",
    "api:generate:mw-storage": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-storage/docs/swagger.json -o ./apiAutogenerated/mw-storage",
    "api:generate:mw-notification": "protoc --proto_path=../mw-notification/proto --go_out=pkg/grpcAutoGenerated/notification --go_opt=paths=source_relative --go-grpc_out=pkg/grpcAutoGenerated/notification --go-grpc_opt=paths=source_relative ../mw-notification/proto/notification.proto",
    "api:generate:mw-chat": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-chat/docs/swagger.json -o ./apiAutogenerated/mw-chat",
    "api:generate:mw-chat-websocket": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-chat-websocket/docs/swagger.json -o ./apiAutogenerated/mw-chat-websocket",
    "api:generate:mw-mail": "openapi-generator-cli generate -g go -t ../go-common/openapi-templates/ -i ../mw-mail/docs/swagger.json -o ./apiAutogenerated/mw-mail",
    "api:generate:mw-training": "protoc --proto_path=../mw-training/proto --experimental_allow_proto3_optional --go_out=pkg/grpcAutoGenerated/training --go_opt=paths=source_relative --go-grpc_out=pkg/grpcAutoGenerated/training --go-grpc_opt=paths=source_relative ../mw-training/proto/training.proto",
    "api:generate:test-websocket": "openapi-generator-cli generate -g typescript-fetch -i ../mw-test-websocket/docs/swagger.json -o ./src/apiAutogenerated/testWebsocket -t ./openApiTemplates"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "3.1.4"
  }
}